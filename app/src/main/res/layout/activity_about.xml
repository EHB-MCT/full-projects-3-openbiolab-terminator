<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity"
    android:background="@drawable/ic_background_wave4">

    <TextView
        android:id="@+id/textView20"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="32dp"
        android:fontFamily="@font/qartella_bold"
        android:text="@string/more_about"
        android:textAllCaps="true"
        android:textColor="#384C71"
        android:textSize="25sp"
        android:textStyle="bold"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/whoButton"
        android:layout_width="390dp"
        android:layout_height="55dp"
        android:layout_marginTop="112dp"
        android:background="@drawable/rectangleround"
        android:backgroundTint="@color/white"
        android:elevation="6dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.476"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/howButton"
        android:layout_width="390dp"
        android:layout_height="55dp"
        android:layout_marginTop="252dp"
        android:background="@drawable/rectangleround"
        android:backgroundTint="@color/white"
        android:elevation="6dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.476"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/whatButton"
        android:layout_width="390dp"
        android:layout_height="55dp"
        android:layout_marginTop="180dp"
        android:background="@drawable/rectangleround"
        android:backgroundTint="@color/white"
        android:elevation="6dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.476"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/whatTextButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="75dp"
        android:layout_marginTop="13dp"
        android:layout_marginEnd="75dp"
        android:layout_marginBottom="16dp"
        android:elevation="6dp"
        android:fontFamily="@font/qartella_regular"
        android:text="What do we do and why?"
        android:textColor="@color/black"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="@+id/whatButton"
        app:layout_constraintEnd_toEndOf="@+id/whatButton"
        app:layout_constraintStart_toStartOf="@+id/whatButton"
        app:layout_constraintTop_toTopOf="@+id/whatButton"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/whoTextButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="134dp"
        android:layout_marginTop="14dp"
        android:layout_marginEnd="133dp"
        android:layout_marginBottom="15dp"
        android:elevation="6dp"
        android:fontFamily="@font/qartella_regular"
        android:text="Who are we?"
        android:textColor="@color/black"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="@+id/whoButton"
        app:layout_constraintEnd_toEndOf="@+id/whoButton"
        app:layout_constraintStart_toStartOf="@+id/whoButton"
        app:layout_constraintTop_toTopOf="@+id/whoButton" />

    <TextView
        android:id="@+id/howTextButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="111dp"
        android:layout_marginTop="14dp"
        android:layout_marginEnd="111dp"
        android:layout_marginBottom="15dp"
        android:elevation="6dp"
        android:fontFamily="@font/qartella_regular"
        android:text="How does it work?"
        android:textColor="@color/black"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="@+id/howButton"
        app:layout_constraintEnd_toEndOf="@+id/howButton"
        app:layout_constraintStart_toStartOf="@+id/howButton"
        app:layout_constraintTop_toTopOf="@+id/howButton" />

    <TextView
        android:id="@+id/navbottom"
        android:layout_width="wrap_content"
        android:layout_height="60dp"
        android:background="@drawable/nav_bottom"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/camera"
        android:layout_width="70dp"
        android:layout_height="51dp"
        android:layout_marginBottom="20dp"
        android:background="@drawable/camera_blue"
        android:backgroundTint="#384C71"
        android:elevation="2dp"
        android:scaleType="fitCenter"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/boomark"
        android:layout_width="21dp"
        android:layout_height="30dp"
        android:layout_marginBottom="20dp"
        android:background="@drawable/saved_blue_empty"
        android:backgroundTint="#384C71"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.265"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/info"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginBottom="20dp"
        android:background="@drawable/info_blue"
        android:backgroundTint="#384C71"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.892"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/profile"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginBottom="20dp"
        android:background="@drawable/profile_blue_empty"
        android:backgroundTint="#384C71"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.73"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/home"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginBottom="20dp"
        android:background="@drawable/home_blue_empty"
        android:backgroundTint="#384C71"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.104"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>