<?xml version="1.0" encoding="utf-8"?>
 <androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/background">

   <TextView
       android:id="@+id/textView5"
       android:layout_width="130dp"
       android:layout_height="170dp"
       android:layout_marginTop="72dp"
       android:background="@drawable/new_logo"
       app:layout_constraintHorizontal_bias="0.498"
       app:layout_constraintLeft_toLeftOf="parent"
       app:layout_constraintRight_toRightOf="parent"
       app:layout_constraintTop_toTopOf="parent" />

   <TextView
       android:id="@+id/textView2"
       android:layout_width="110dp"
       android:layout_height="56dp"
       android:layout_marginTop="101dp"
       android:fontFamily="@font/qartella_bold"
       android:gravity="center"
       android:text="@string/signup"
       android:textColor="@color/white"
       android:textSize="28sp"
       android:textStyle="bold"
       app:layout_constraintEnd_toEndOf="parent"
       app:layout_constraintHorizontal_bias="0.498"
       app:layout_constraintStart_toStartOf="parent"
       app:layout_constraintTop_toBottomOf="@+id/textView5" />

   <TextView
       android:id="@+id/rectlog1"
       android:layout_width="340dp"
       android:layout_height="40dp"
       android:layout_marginTop="12dp"
       android:background="@drawable/rectangle"
       android:fontFamily="@font/qartella_bold"
       app:layout_constraintEnd_toEndOf="parent"
       app:layout_constraintHorizontal_bias="0.493"
       app:layout_constraintStart_toStartOf="parent"
       app:layout_constraintTop_toBottomOf="@+id/textView2" />

    <TextView
        android:id="@+id/rectlog2"
        android:layout_width="340dp"
        android:layout_height="40dp"
        android:layout_marginTop="12dp"
        android:layout_marginBottom="368dp"
        android:background="@drawable/rectangle"
        android:fontFamily="@font/qartella_bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.492"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/rectlog1"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
       android:id="@+id/rectlog3"
       android:layout_width="340dp"
       android:layout_height="40dp"
       android:layout_marginTop="10dp"
       android:background="@drawable/rectangle"
       android:fontFamily="@font/qartella_bold"
       app:layout_constraintEnd_toEndOf="parent"
       app:layout_constraintHorizontal_bias="0.493"
       app:layout_constraintStart_toStartOf="parent"
       app:layout_constraintTop_toBottomOf="@+id/rectlog2" />


   <EditText
       android:id="@+id/editTextTextEmailAddress"
       android:layout_width="wrap_content"
       android:layout_height="48dp"
       android:layout_marginEnd="1dp"
       android:layout_marginBottom="2dp"
       android:contentDescription="Email"
       android:ems="10"
       android:fontFamily="@font/qartella_regular"
       android:gravity=""
       android:inputType="textEmailAddress"
       android:text="Email"
       android:textColor="#90000000"
       android:textSize="20sp"
       app:layout_constraintBottom_toBottomOf="@+id/rectlog2"
       app:layout_constraintEnd_toEndOf="@+id/rectlog2"
       app:layout_constraintHorizontal_bias="0.0"
       app:layout_constraintStart_toStartOf="@+id/rectlog2"
       app:layout_constraintTop_toTopOf="@+id/rectlog2"
       app:layout_constraintVertical_bias="0.0" />

   <EditText
       android:id="@+id/editTextTextPassword"
       android:layout_width="wrap_content"
       android:layout_height="48dp"
       android:layout_marginTop="12dp"
       android:layout_marginEnd="1dp"
       android:contentDescription="Password"
       android:ems="10"
       android:fontFamily="@font/qartella_regular"
       android:inputType="textPassword"
       android:text="Password"
       android:textColor="#90000000"
       android:textSize="20sp"
       android:gravity=""
       app:layout_constraintBottom_toBottomOf="@+id/rectlog3"
       app:layout_constraintEnd_toEndOf="@+id/rectlog3"
       app:layout_constraintHorizontal_bias="0.0"
       app:layout_constraintStart_toStartOf="@+id/rectlog3"
       app:layout_constraintTop_toTopOf="@+id/rectlog3"
       app:layout_constraintVertical_bias="1.0" />

    <EditText
        android:id="@+id/editTextTextPersonName"
        android:layout_width="wrap_content"
        android:layout_height="48dp"
        android:layout_marginEnd="96dp"
        android:layout_marginBottom="2dp"
        android:contentDescription="Username"
        android:ems="10"
        android:fontFamily="@font/qartella_regular"
        android:gravity=""
        android:inputType="textPersonName"
        android:text="Username"
        android:textColor="#90000000"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="@+id/rectlog1"
        app:layout_constraintEnd_toEndOf="@+id/rectlog1"
        app:layout_constraintTop_toTopOf="@+id/rectlog1"
        app:layout_constraintVertical_bias="0.8" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/button2"
        android:layout_width="293dp"
        android:layout_height="42dp"
        android:layout_marginTop="648dp"
        android:background="@drawable/buttonblue"
        android:fontFamily="@font/qartella_bold"
        android:text="@string/signup"
        android:textAllCaps="false"
        android:textColor="@color/white"
        android:textSize="20sp"
       android:gravity="center"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/signup1"
        android:layout_width="236dp"
        android:layout_height="29dp"
        android:layout_marginTop="704dp"
        android:gravity="center"
        android:fontFamily="@font/qartella_regular"
        android:text="@string/signuppage_login"
        android:textAllCaps="false"
        android:textColor="@color/white"
        android:textSize="15sp"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="50dp"
        android:layout_height="18dp"
        android:layout_marginBottom="40dp"
        android:text="@string/goback"
        android:fontFamily="@font/qartella_regular"
        android:textColor="@color/white"
        android:textSize="12sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent" />


</androidx.constraintlayout.widget.ConstraintLayout>